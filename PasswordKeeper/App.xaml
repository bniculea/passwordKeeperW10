<Application
    x:Class="PasswordKeeper.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:PasswordKeeper"
    RequestedTheme="Dark">
        <Application.Resources>
            <ResourceDictionary>
                <x:String x:Key="AppName">PASSWORD MANAGER</x:String>
                <x:String x:Key="AddLabel">Add it</x:String>
                <LinearGradientBrush x:Key="BrownGradientBrush"
                                     EndPoint="0.5,1"
                                     StartPoint="0.5,0">
                    <GradientStop Color="Black"
                                  Offset="0"/>
                <GradientStop Color="#FFE8B8B8" Offset="1"/>
            </LinearGradientBrush>
            <Color x:Key="DisplayDataBackgroundColor">#FF816363</Color>
            </ResourceDictionary>
        </Application.Resources>
</Application>
